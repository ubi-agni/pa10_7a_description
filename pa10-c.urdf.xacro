<?xml version="1.0"?>
<robot name="PA10c"
		 xmlns:xacro="http://www.ros.org/wiki/xacro"
		 xmlns:sensor="http://playerstage.sourceforge.net/gazebo/xmlschema/#sensor"
		 xmlns:controller="http://playerstage.sourceforge.net/gazebo/xmlschema/#controller"
       xmlns:interface="http://playerstage.sourceforge.net/gazebo/xmlschema/#interface">

  <xacro:include filename="$(find agni_description)/urdf/util.urdf.xacro"/>

  <macro name="pa10c_arm" params="name">
  <link name="${name}arm_base_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://agni_description/pa10/meshes/pa-7c/link0_mesh.dae" scale="1 1 1" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0.115" rpy="0 0 0"/>
      <geometry>
		  <cylinder length="0.200" radius="0.112"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
  </link>
  <link name="${name}arm_1_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://agni_description/pa10/meshes/pa-7c/link1_mesh.dae" scale="1 1 1" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://agni_description/pa10/meshes/pa-7c/link1_mesh.dae" scale="1 1 1" />
      </geometry>
    </collision>
    <inertial>
      <mass value="1" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
  </link>
  <link name="${name}arm_2_link">
    <visual>
      <origin xyz="0 0 -0.317" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://agni_description/pa10/meshes/pa-7c/link2_mesh.dae" scale="1 1 1" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 -0.317" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://agni_description/pa10/meshes/pa-7c/link2_mesh.dae" scale="1 1 1" />
      </geometry>
    </collision>
    <inertial>
      <mass value="1" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
  </link>
  <link name="${name}arm_3_link">
    <visual>
      <origin xyz="0 0 -0.317" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://agni_description/pa10/meshes/pa-7c/link3_mesh.dae" scale="1 1 1" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 -0.317" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://agni_description/pa10/meshes/pa-7c/link3_mesh.dae" scale="1 1 1" />
      </geometry>
    </collision>
    <inertial>
      <mass value="1" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
  </link>
  <link name="${name}arm_4_link">
    <visual>
      <origin xyz="0 0 -0.767" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://agni_description/pa10/meshes/pa-7c/link4_mesh.dae" scale="1 1 1" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 -0.767" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://agni_description/pa10/meshes/pa-7c/link4_mesh.dae" scale="1 1 1" />
      </geometry>
    </collision>
    <inertial>
      <mass value="1" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
  </link>
  <link name="${name}arm_5_link">
    <visual>
      <origin xyz="0 0 -0.767" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://agni_description/pa10/meshes/pa-7c/link5_mesh.dae" scale="1 1 1" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 -0.767" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://agni_description/pa10/meshes/pa-7c/link5_mesh.dae" scale="1 1 1" />
      </geometry>
    </collision>
    <inertial>
      <mass value="1" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
  </link>
  <link name="${name}arm_6_link">
    <visual>
      <origin xyz="0 0 -1.247" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://agni_description/pa10/meshes/pa-7c/link6_mesh.dae" scale="1 1 1" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 -1.247" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://agni_description/pa10/meshes/pa-7c/link6_mesh.dae" scale="1 1 1" />
      </geometry>
    </collision>
    <inertial>
      <mass value="1" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
  </link>
  <link name="${name}arm_7_link">
    <visual>
      <origin xyz="0 0 1.317" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://agni_description/pa10/meshes/pa-7c/link7_mesh.dae" scale="1 1 1" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 1.317" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://agni_description/pa10/meshes/pa-7c/link7_mesh.dae" scale="1 1 1" />
      </geometry>
    </collision>
    <inertial>
      <mass value="1" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
  </link>

  <joint name="${name}S1" type="revolute">
    <parent link="${name}arm_base_link"/>
    <child  link="${name}arm_1_link"/>
    <origin xyz="0 0 0" rpy="0 0 0 "/>
    <axis   xyz="0 0 1"/>
    <limit lower="${-177/180*M_PI}" upper="${177/180*M_PI}" effort="100" velocity="0.5" />
    <dynamics damping="0.2" friction="0" />
  </joint>
  <joint name="${name}S2" type="revolute">
    <parent link="${name}arm_1_link"/>
    <child  link="${name}arm_2_link"/>
    <origin xyz="0 0 0.317" rpy="0 0 0 "/>
    <axis   xyz="0 1 0"/>
    <limit lower="${-94/180*M_PI}" upper="${+94/180*M_PI}" effort="100" velocity="0.5" />
    <dynamics damping="0.2" friction="0" />
  </joint>
  <joint name="${name}S3" type="revolute">
    <parent link="${name}arm_2_link"/>
    <child  link="${name}arm_3_link"/>
    <origin xyz="0 0 0" rpy="0 0 0 "/>
    <axis   xyz="0 0 1"/>
    <limit lower="${-174/180*M_PI}" upper="${174/180*M_PI}" effort="100" velocity="0.5" />
    <dynamics damping="0.2" friction="0" />
  </joint>
  <joint name="${name}E1" type="revolute">
    <parent link="${name}arm_3_link"/>
    <child  link="${name}arm_4_link"/>
    <origin xyz="0 0 0.45" rpy="0 0 0 "/>
    <axis   xyz="0 1 0"/>
    <limit lower="${-137/180*M_PI}" upper="${137/180*M_PI}" effort="100" velocity="0.5" />
    <dynamics damping="0.2" friction="0" />
  </joint>
  <joint name="${name}E2" type="revolute">
    <parent link="${name}arm_4_link"/>
    <child  link="${name}arm_5_link"/>
    <origin xyz="0 0 0" rpy="0 0 0 "/>
    <axis   xyz="0 0 1"/>
    <limit lower="${-255/180*M_PI}" upper="${255/180*M_PI}" effort="100" velocity="0.5" />
    <dynamics damping="0.2" friction="0" />
  </joint>
  <joint name="${name}W1" type="revolute">
    <parent link="${name}arm_5_link"/>
    <child  link="${name}arm_6_link"/>
    <origin xyz="0 0 0.480" rpy="0 0 0 "/>
    <axis   xyz="0 1 0"/>
    <limit lower="${-165/180*M_PI}" upper="${165/180*M_PI}" effort="100" velocity="0.5" />
    <dynamics damping="0.2" friction="0" />
  </joint>
  <joint name="${name}W2" type="revolute">
    <parent link="${name}arm_6_link"/>
    <child  link="${name}arm_7_link"/>
    <origin xyz="0 0 0.07" rpy="0 0 0 "/>
    <axis   xyz="0 0 1"/>
    <limit lower="${-255/180*M_PI}" upper="${255/180*M_PI}" effort="100" velocity="0.5" />
    <dynamics damping="0.2" friction="0" />
  </joint>

  <joint name="${name}arm_hand_fixed_joint" type="fixed">
	 <parent link="${name}arm_7_link"/>
	 <child link="${name}arm_hand_mount"/>
  </joint>
  
  <link name="${name}arm_hand_mount" />
  </macro>
</robot>
